<?xml version="1.0" encoding="utf-8"?>

<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/ScrollView2"
    android:layout_width="match_parent"
    android:layout_height="match_parent">


    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:layout_gravity="center"
        android:orientation="vertical">

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_gravity="center"
            android:layout_marginTop="30dp"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/search_key"
                android:layout_width="wrap_content"
                android:layout_height="47dp"
                android:layout_weight="3"
                android:gravity="center_vertical"
                android:text="방 자세히 보기"
                android:textColor="#131212"
                android:textSize="15sp" />


            <EditText
                android:id="@+id/test"
                android:layout_width="100dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:text=""
                android:textSize="30sp" />

            <Button

                android:id="@+id/login_btn"

                android:layout_width="wrap_content"

                android:layout_height="wrap_content"

                android:layout_weight="1"
                android:text="리뷰쓰기" />





        </LinearLayout>



        <LinearLayout

            android:layout_width="wrap_content"

            android:layout_height="match_parent"

            android:layout_gravity="center"

            android:orientation="vertical">



            <TextView

                android:id="@+id/pricce"

                android:layout_width="372dp"

                android:layout_height="wrap_content"

                android:layout_gravity="center"

                android:gravity="center_horizontal"

                android:text="방 가격(월세 100/90)"

                android:textColor="#090808"

                android:textSize="30sp" />



            <ImageView

                android:id="@+id/imageView"

                android:layout_width="369dp"

                android:layout_height="214dp"

                android:layout_gravity="center"

                tools:srcCompat="@drawable/testimg" />


        </LinearLayout>

        <LinearLayout
            android:layout_width="369dp"
            android:layout_height="40dp"
            android:layout_gravity="center">

            <TextView
                android:id="@+id/textView"
                android:layout_width="110dp"
                android:layout_height="40dp"
                android:layout_gravity="center"
                android:text="주소"
                android:textColor="#111010"
                android:textSize="23sp" />

            <TextView
                android:id="@+id/editText"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:ems="10"
                android:inputType="textPersonName"
                android:text="경기도 용인시 모현면" />


        </LinearLayout>

        <TextView
            android:id="@+id/textView11"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="" />
        <TextView
            android:id="@+id/specefic"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            />
        <LinearLayout
            android:layout_width="369dp"
            android:layout_height="40dp"
            android:layout_gravity="center"
            >

            <TextView
                android:id="@+id/textView1"
                android:layout_width="110dp"
                android:layout_height="40dp"
                android:layout_gravity="center"
                android:text="가격"
                android:textColor="#111010"
                android:textSize="23sp" />

            <TextView
                android:id="@+id/editText6"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:ems="10"
                android:inputType="textPersonName"
                android:text="월세 100/90" />

        </LinearLayout>
        <LinearLayout
            android:layout_width="369dp"
            android:layout_height="40dp"
            android:layout_gravity="center"
            >

            <TextView
                android:id="@+id/textView2"
                android:layout_width="110dp"
                android:layout_height="40dp"
                android:layout_gravity="center"
                android:text="층수"
                android:textColor="#111010"
                android:textSize="23sp" />

            <TextView
                android:id="@+id/editText2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:ems="10"
                android:inputType="textPersonName"
                android:text="1층" />

        </LinearLayout>
        <LinearLayout
            android:layout_width="369dp"
            android:layout_height="40dp"
            android:layout_gravity="center"
            >

            <TextView
                android:id="@+id/textView3"
                android:layout_width="110dp"
                android:layout_height="40dp"
                android:layout_gravity="center"
                android:text="방크기"
                android:textColor="#111010"
                android:textSize="23sp" />

            <TextView
                android:id="@+id/editText3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:ems="10"
                android:inputType="textPersonName"
                android:text="24평" />

        </LinearLayout>
        <LinearLayout
            android:layout_width="369dp"
            android:layout_height="40dp"
            android:layout_gravity="center"
            >

            <TextView
                android:id="@+id/textView4"
                android:layout_width="110dp"
                android:layout_height="40dp"
                android:layout_gravity="center"
                android:text="옵션"
                android:textColor="#111010"
                android:textSize="23sp" />

            <TextView
                android:id="@+id/editText4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:ems="10"
                android:inputType="textPersonName"
                android:text="세탁기, 냉장고, 에어컨 있음" />

        </LinearLayout>
        <LinearLayout
            android:layout_width="369dp"
            android:layout_height="40dp"
            android:layout_gravity="center"
            >

            <TextView
                android:id="@+id/textView5"
                android:layout_width="110dp"
                android:layout_height="40dp"
                android:layout_gravity="center"
                android:text="관리비"
                android:textColor="#111010"
                android:textSize="23sp" />

            <TextView
                android:id="@+id/editText5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:ems="10"
                android:inputType="textPersonName"
                android:text="없음" />

        </LinearLayout>
        <LinearLayout
            android:layout_width="369dp"
            android:layout_height="40dp"
            android:layout_gravity="center"
            >

            <TextView
                android:id="@+id/textView6"
                android:layout_width="110dp"
                android:layout_height="40dp"
                android:layout_gravity="center"
                android:text="주차비"
                android:textColor="#111010"
                android:textSize="23sp" />

            <TextView
                android:id="@+id/editText1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:ems="10"
                android:inputType="textPersonName"
                android:text="없음" />

        </LinearLayout>
        <LinearLayout
            android:layout_width="369dp"
            android:layout_height="40dp"
            android:layout_gravity="center"
            >

            <TextView
                android:id="@+id/textView7"
                android:layout_width="110dp"
                android:layout_height="40dp"
                android:layout_gravity="center"
                android:text="임대기간"
                android:textColor="#111010"
                android:textSize="23sp" />

            <TextView
                android:id="@+id/editText8"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:ems="10"
                android:inputType="textPersonName" />

        </LinearLayout>
        <LinearLayout
            android:layout_width="369dp"
            android:layout_height="40dp"
            android:layout_gravity="center"
            >

            <TextView
                android:id="@+id/textView8"
                android:layout_width="110dp"
                android:layout_height="40dp"
                android:layout_gravity="center"
                android:layout_weight="1"
                android:text="입주가능일"
                android:textColor="#111010"
                android:textSize="23sp" />

            <TextView
                android:id="@+id/textView10"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:text="TextView" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/textView9"
                android:layout_width="110dp"
                android:layout_height="40dp"
                android:layout_gravity="center"
                android:layout_weight="1"
                android:text="     방 종류"
                android:textColor="#111010"
                android:textSize="23sp" />

            <TextView
                android:id="@+id/textView12"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:text="TextView" />


        </LinearLayout>

        <TextView
            android:id="@+id/editText7"
            android:layout_width="369dp"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:background="@null"
            android:ems="10"
            android:inputType="textPersonName"
            android:text="한국외대 글로벌캠퍼스 주변 단기 임대합니다." />


        <fragment

            android:id="@+id/map"

            android:name="com.naver.maps.map.MapFragment"

            android:layout_width="match_parent"

            android:layout_height="400dp" />





        <Button

            android:id="@+id/call_btn"

            android:layout_width="match_parent"

            android:layout_height="wrap_content"

            android:text="메세지로 연락하기" />


        <Button

            android:id="@+id/home_btn"

            android:layout_width="match_parent"

            android:layout_height="wrap_content"
            android:text="홈으로 돌아가기" />



    </LinearLayout>

</ScrollView>